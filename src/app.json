    "pages/addQun/addQun", // 加入群
    "pages/user/user", // 我的
    "pages/addressList/addressList", // 通讯录
    "pages/myQun/myQun", // 用户已加群详情
    "pages/writeQun/writeQun", // 发布群友圈
    "pages/qunMessageDetail/qunMessageDetail", // 群消息详情
    "pages/userHelp/userHelp", // 新手帮助
    "pages/createQun/createQun", // 创建社群
    "pages/search/search", // 搜索
    "pages/qunClassify/qunClassify", // 群分类
    "pages/wallet/wallet", // 钱包
    "pages/classify/classify", // 社群分类
    "pages/permission/permission", // 权限控制
    "pages/createNameCard/createNameCard", // 创建名片
    "pages/aboutQun/aboutQun", // 关于群信息
